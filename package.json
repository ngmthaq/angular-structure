{
  "name": "angular-structure",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ng": "ng",
    "ng:c": "node ./scripts/generate-component.script.js",
    "ng:p": "node ./scripts/generate-page.script.js",
    "ng:m": "node ./scripts/generate-module.script.js",
    "ng:l": "node ./scripts/generate-layout.script.js",
    "ng:env": "cp ./src/environments/environment.private.example.ts ./src/environments/environment.private.ts",
    "ng:dev": "ng serve",
    "ng:build": "ng build",
    "ng:watch": "ng build --watch --configuration development",
    "test:unit": "ng test --watch=false --browsers=ChromeHeadless",
    "test:e2e": "ng e2e",
    "node:start": "cross-env NODE_ENV=production node ./server/bin/app.js",
    "node:watch": "cross-env NODE_ENV=development nodemon --watch dist --watch server server/bin/app.js",
    "node:env": "cp ./server/.env.example ./server/.env",
    "convention:fix": "prettier . --write",
    "convention:lint": "prettier . --check && eslint src && eslint e2e/tests",
    "prepare": "husky",
    "lint-staged": "npm run convention:lint",
    "start": "npm run ng:build && npm run node:start",
    "dev": "concurrently \"npm run ng:watch\" \"npm run node:watch\""
  },
  "lint-staged": {
    "**/*": "npm run lint-staged"
  },
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "axios": "^1.6.8",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.11",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "humps": "^2.0.1",
    "isbot": "^5.1.9",
    "js-cookie": "^3.0.5",
    "localforage": "^1.10.0",
    "morgan": "~1.9.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular/cli": "^17.3.7",
    "@angular/compiler-cli": "^17.3.0",
    "@eslint/js": "^9.3.0",
    "@puppeteer/ng-schematics": "0.6.0",
    "@types/humps": "^2.0.6",
    "@types/jasmine": "~5.1.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "20.12.12",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "9.x",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.3.0",
    "husky": "^9.0.11",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.5",
    "nodemon": "^3.1.3",
    "postcss": "^8.4.38",
    "prettier": "3.2.5",
    "puppeteer": "22.10.0",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.5",
    "typescript-eslint": "^7.10.0"
  },
  "engines": {
    "node": ">=20.11.1"
  }
}
